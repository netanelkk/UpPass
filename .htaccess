RewriteEngine On
RewriteRule ^upload$ index.php?do=upload [L,NC,QSA]
RewriteRule ^tools$ index.php?do=tools [L,NC,QSA]
RewriteRule ^logout$ action.php?do=logout [L,NC,QSA]
RewriteRule ^myfiles$ index.php?do=myfiles [L,NC,QSA]
RewriteRule ^register$ index.php?do=register [L,NC,QSA]
RewriteRule ^settings$ index.php?do=settings [L,NC,QSA]
RewriteRule ^contact$ index.php?do=contact [L,NC,QSA]
RewriteRule ^admin$ index.php?do=admin [L,NC,QSA]
RewriteRule ^forgot$ index.php?do=forgot [L,NC,QSA]
