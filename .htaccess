RewriteRule ^links/(.*)$ index.php?do=links&id=$1 [L,NC,QSA]
RewriteRule ^active/(.*)$ index.php?do=active&id=$1 [L,NC,QSA]
RewriteRule ^gallery/(.*)$ index.php?do=gallery&id=$1 [L,NC,QSA]
RewriteRule ^restore/(.*)$ index.php?do=restore&id=$1 [L,NC,QSA]
